{"ast":null,"code":"import AppProductList from './components/ProductList.vue';\nimport products from './data/products';\nimport AppBasePagination from './components/BasePagination.vue';\nexport default {\n  name: 'App',\n  components: {\n    AppProductList,\n    AppBasePagination\n  },\n\n  data() {\n    return {\n      page: 1,\n      productsPerPage: 6\n    };\n  },\n\n  computed: {\n    products() {\n      const offset = (this.page - 1) * this.productsPerPage;\n      return products.slice(offset, offset + this.productsPerPage);\n    },\n\n    countProducts() {\n      return products.length;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAcA;AACA;AACA;AAEA;EACAA,WADA;EAEAC;IACAC,cADA;IAEAC;EAFA,CAFA;;EAOAC;IACA;MACAC,OADA;MAEAC;IAFA;EAIA,CAZA;;EAcAC;IACAC;MACA;MACA;IACA,CAJA;;IAKAC;MACA;IACA;;EAPA;AAdA","names":["name","components","AppProductList","AppBasePagination","data","page","productsPerPage","computed","products","countProducts"],"sourceRoot":"src","sources":["App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <section class=\"catalog\">\n      <app-product-list :products=\"products\"></app-product-list>\n      <app-base-pagination\n        v-model=\"page\", ,\n        :count=\"countProducts\"\n        :per-page=\"productsPerPage\"\n      ></app-base-pagination>\n    </section>\n  </div>\n</template>\n\n<script>\nimport AppProductList from './components/ProductList.vue'\nimport products from './data/products'\nimport AppBasePagination from './components/BasePagination.vue'\n\nexport default {\n  name: 'App',\n  components: {\n    AppProductList,\n    AppBasePagination,\n  },\n\n  data() {\n    return {\n      page: 1,\n      productsPerPage: 6,\n    }\n  },\n\n  computed: {\n    products() {\n      const offset = (this.page - 1) * this.productsPerPage\n      return products.slice(offset, offset + this.productsPerPage)\n    },\n    countProducts() {\n      return products.length\n    },\n  },\n}\n</script>\n\n<style></style>\n"]},"metadata