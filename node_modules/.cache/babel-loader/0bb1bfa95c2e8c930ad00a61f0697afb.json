{"ast":null,"code":"import AppProductList from './components/ProductList.vue';\nimport products from './data/products';\nimport AppBasePagination from './components/BasePagination.vue';\nexport default {\n  name: 'App',\n  components: {\n    AppProductList,\n    AppBasePagination,\n    AppProductFilter\n  },\n\n  data() {\n    return {\n      page: 1,\n      productsPerPage: 6\n    };\n  },\n\n  computed: {\n    products() {\n      const offset = (this.page - 1) * this.productsPerPage;\n      return products.slice(offset, offset + this.productsPerPage);\n    },\n\n    countProducts() {\n      return products.length;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA0BA;AACA;AACA;AAGA;EACAA,WADA;EAEAC;IACAC,cADA;IAEAC,iBAFA;IAGAC;EAHA,CAFA;;EAQAC;IACA;MACAC,OADA;MAEAC;IAFA;EAIA,CAbA;;EAeAC;IACAC;MACA;MACA;IACA,CAJA;;IAKAC;MACA;IACA;;EAPA;AAfA","names":["name","components","AppProductList","AppBasePagination","AppProductFilter","data","page","productsPerPage","computed","products","countProducts"],"sourceRoot":"src","sources":["App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <main class=\"content container\">\n      <div class=\"content__top content__top--catalog\">\n        <h1 class=\"content__title\">Каталог</h1>\n        <span class=\"content__info\"> 152 товара </span>\n      </div>\n\n      <div class=\"content__catalog\">\n       <app-product-filter></app-product-filter>\n\n        <section class=\"catalog\">\n          <app-product-list :products=\"products\"></app-product-list>\n\n          <app-base-pagination\n            v-model=\"page\"\n            :count=\"countProducts\"\n            :per-page=\"productsPerPage\"\n          ></app-base-pagination>\n        </section>\n      </div>\n    </main>\n  </div>\n</template>\n\n<script>\nimport AppProductList from './components/ProductList.vue'\nimport products from './data/products'\nimport AppBasePagination from './components/BasePagination.vue'\n\n\nexport default {\n  name: 'App',\n  comp